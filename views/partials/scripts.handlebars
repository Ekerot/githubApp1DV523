<script src="https://code.jquery.com/jquery-3.1.1.js"
        integrity="sha256-16cdPddA6VdVInumRGo6IbivbERE8p7CQR3HzTBuELA="
        crossorigin="anonymous"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.js"></script>

<script type="text/javascript">$(document).ready(function(){
    $('.collapsible').collapsible();});</script>

<script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>

<script>
    var socket = io.connect();

    socket.on('webhook', function(hook){
        console.log(hook)
        $('#list-issues').append($('<li>').text(JSON.stringify(hook.action)));
    });
</script>

<li id="{{this.id}} list-element">
    <div class="collapsible-header red darken-1 white-text z-depth-5"><i class="material-icons">whatshot</i>{{this.title}}</div>
    <div class="collapsible-body"><p>{{this.body}}</p>
        <p>
            {{this.comments}} many comments are written</p>
        <p><a href="{{this.html_url}}">{{this.html_url}}</a></p>
        <p>created at {{this.created_at}} and by <div class="chip">
            <img class="round" src="{{this.avatar_url}}">
            <span class="card-title">{{this.login}}</span>
        </div></p>
    </div>
</li>