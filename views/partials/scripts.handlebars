<script src="https://code.jquery.com/jquery-3.1.1.js"
        integrity="sha256-16cdPddA6VdVInumRGo6IbivbERE8p7CQR3HzTBuELA="
        crossorigin="anonymous"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.js"></script>

<script type="text/javascript">$(document).ready(function(){
    $('.collapsible').collapsible();});</script>

<script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>

<script>
    var socket = io.connect();

    socket.on('webhook', function(hook){

        $('#list-issues').append($('<li>').attr("id", hook.issue.id + " list-element"));
        $('#list-element').append($('<div>').attr('class', "collapsible-header red darken-1 white-text z-depth-5").
        append($('<i>').attr('class', 'material-icons').text('whatshot')));
        $('#list-element').append($('<div>').attr('class', "collapsible-body").
        append($('<p>').text(hook.issue.comments + ' many comments are written')));
        $('.collapsible-body').append($('<div>').attr('class', "collapsible-body").
        append($('<p>').text('created at ' + hook.created_at + 'and by ').appendChild($('<div>').attr('class', 'chip').text(hook.issue.html_url))));
        $('.collapsible-body').append($('<div>').attr('class', "collapsible-body").
        append($('<p>').text(hook.issue.comments + ' many comments are written')));

    });
</script>

